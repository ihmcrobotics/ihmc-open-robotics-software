      const octet CONFIGURATION_SPACE_NAME_X =
      0;

      const octet CONFIGURATION_SPACE_NAME_Y =
      1;

      const octet CONFIGURATION_SPACE_NAME_Z =
      2;

      const octet CONFIGURATION_SPACE_NAME_ROLL =
