\begin_layout Title
A novel state estimator for floating base robots
\end_layout

\begin_layout Author
Twan Koolen, Jerry Pratt
\end_layout

\begin_layout Standard
\begin_inset Newpage clearpage
\end_inset


\end_layout

\begin_layout Standard
\begin_inset CommandInset toc
LatexCommand tableofcontents

\end_inset


